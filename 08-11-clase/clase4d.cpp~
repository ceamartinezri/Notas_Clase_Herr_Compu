#include<iostream>
//Comportamiento de punto Flotante - FLoat XS
int main()
{
  double under=1.0;
  double over=1.0;
  
  int n=1100;

  for(int i = 1; i <= n; i += 1){
    under= under/2.0f;
    over = over*2.0f;
    std::cout<< "under " << i << ": " << under << "  - over "<< i << ": "<<  over <<std::endl;
  }
return 0;
}
